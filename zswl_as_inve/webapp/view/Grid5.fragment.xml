<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout">
	<VBox height="17rem">
		<VBox>
			<FlexBox justifyContent="SpaceBetween">
				<items>
					<Text text="{i18n>Form_Year}" class="customTextSmall"/>
					<Text text="{PIDoc>/DocYear}" class="customTextSmall"/>
				</items>
			</FlexBox>
			<FlexBox justifyContent="SpaceBetween">
				<items>
					<Text text="{i18n>Form_Docno}" wrapping="false" class="customTextSmall"/>
					<Text text="{PIDoc>/DocNumber}" wrapping="false" class="customTextSmall"/>
				</items>
			</FlexBox>
			<FlexBox justifyContent="SpaceBetween">
				<items>
					<Text text="{i18n>Form_Type}" wrapping="false" class="customTextSmall"/>
					<Text text="{PIDoc>/DocType}" wrapping="false" class="customTextSmall"/>
				</items>
			</FlexBox>
			<FlexBox justifyContent="SpaceBetween">
				<items>
					<Text text="{i18n>Form_Aarea}" wrapping="false" class="customTextSmall"/>
					<Text text="{PIDoc>/Aarea}" wrapping="false" class="customTextSmall"/>
				</items>
			</FlexBox>
		</VBox>
	</VBox>

</core:FragmentDefinition>