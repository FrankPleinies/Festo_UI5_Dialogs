<mvc:View controllerName="zswl_as_inve.controller.Main" xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout" xmlns:table="sap.ui.table"
	xmlns:form="sap.ui.layout.form" xmlns="sap.m" displayBlock="true">
	<!-- <Shell id="shell"> -->
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>dialog_title}" showFooter="true"
					class="sapUiResponsivePadding--header sapUiResponsivePadding--subHeader sapUiResponsivePadding--footer customContentLayout customHeader">
					<headerContent>
						<Text id="user" text="{i18n>header_user}" class="headerUser"/>
						<Image src="./img/swllogo.png" width="100px"/>
					</headerContent>
					<content>
						<l:BlockLayout id="BlockLayout" background="Dashboard">
							<!-- first row -->
							<l:BlockLayoutRow accentCells="Accent1">
								<!-- first cell-->
								<l:BlockLayoutCell class="smallPaddingCustom customCellHeader" id="Grid1" width="1" title="{i18n>Grid1_Title}">
									<core:Fragment xmlns:core="sap.ui.core" fragmentName="zswl_as_inve.view.Grid1" type="XML"/>
								</l:BlockLayoutCell>
								<!-- second cell-->
								<l:BlockLayoutCell class="smallPaddingCustom customCellHeader" id="Grid2" title="{i18n>Grid2_Title}">
									<core:Fragment xmlns:core="sap.ui.core" fragmentName="zswl_as_inve.view.Grid2" type="XML"/>
								</l:BlockLayoutCell>
								<!-- third cell-->
								<l:BlockLayoutCell class="smallPaddingCustom customTopBorder customCellHeader" id="Grid3" title="{i18n>Grid3_Title}">
									<core:Fragment xmlns:core="sap.ui.core" fragmentName="zswl_as_inve.view.Grid3" type="XML"/>
								</l:BlockLayoutCell>
							</l:BlockLayoutRow>
							<!-- second row -->
							<l:BlockLayoutRow>
								<!-- forth cell-->
								<l:BlockLayoutCell class="smallPaddingCustom customCellHeader" id="Grid4" title="{i18n>Grid4_Title}">
									<core:Fragment xmlns:core="sap.ui.core" fragmentName="zswl_as_inve.view.Grid4" type="XML"/>
								</l:BlockLayoutCell>
								<!-- fifth cell-->
								<l:BlockLayoutCell class="smallPaddingCustom customCellHeader" id="Grid5"  title="{i18n>Grid5_Title}">
									<core:Fragment xmlns:core="sap.ui.core" fragmentName="zswl_as_inve.view.Grid5" type="XML"/>
								</l:BlockLayoutCell>
								<!-- sixth cell-->
								<l:BlockLayoutCell class="smallPaddingCustom customCellHeader  customButtomBorder" id="Grid6">
									<core:Fragment xmlns:core="sap.ui.core" fragmentName="zswl_as_inve.view.Grid6" type="XML"/>
								</l:BlockLayoutCell>
							</l:BlockLayoutRow>
						</l:BlockLayout>

					</content>
					<footer>
						<OverflowToolbar height="70px">
							<Button icon="sap-icon://arrow-top" text="{i18n>Btn_Exit}" press="onExitPressed" class="customFooterButton"/>
							<Button text="{i18n>Btn_ReqMan}" press="onRequestPressed" class="customFooterButton" id="btn_reqman" enabled="{ELEMENTS>/btn_reqman/enabled}" />
							<HBox width="70%" id="hbox1" justifyContent="Center" alignItems="Stretch" alignContent="Stretch" class="sapUiSmallMarginEnd"/>
							<Button icon="sap-icon://alert" text="{= ${message>/}.length }" visible="true" class="sapUiSmallMarginEnd customFooterButton"
								press="onMessagePopoverPress"/>
						</OverflowToolbar>
					</footer>

				</Page>
			</pages>
		</App>
	<!-- </Shell> -->
</mvc:View>