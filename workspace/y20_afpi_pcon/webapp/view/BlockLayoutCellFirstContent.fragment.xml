<core:FragmentDefinition
	xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:l="sap.ui.layout"
	xmlns:table="sap.ui.table"
	xmlns:form="sap.ui.layout.form"
	xmlns="sap.m">
	<VBox height="30rem" id="firstRowVBox" justifyContent="Start">

		<VBox height="100%" justifyContent="Start">
			<HBox width="100%" justifyContent="SpaceBetween" alignItems="Stretch" alignContent="Stretch" class="customInnerCellHeaderBox">
				<Text class="customInnerCellHeaderText" text="{i18n>mainScreenFirstCellTitle}"/>
				<Text class="customInnerCellHeaderText" id="sourceHUTitle" text="{modelSourceHU>/HUIdent}"/>
			</HBox>
			<!-- Source HU -->
			<FlexBox class="sapUiTinyMarginTop" justifyContent="SpaceBetween">
				<Text class="sapUiTinyMarginBegin sapUiTinyMarginTop customTextSmall" text="{i18n>mainScreenFieldSourceHU}"/>
				<Text class="sapUiTinyMarginEnd sapUiTinyMarginTop customTextLarge" id="sourceHUValue" text="{modelSourceHU>/HUIdent}"/>
			</FlexBox>
			<!-- WarehouseOrder: EWM OD Document -->
			<FlexBox class="sapUiTinyMarginTop" justifyContent="SpaceBetween">
				<Text class="sapUiTinyMarginBegin sapUiTinyMarginTop customTextSmall" text="{i18n>formWarehouseOrderField1}"/>
				<Text class="sapUiTinyMarginEnd sapUiTinyMarginTop customTextSmall" text="{modelWarehouseOrder>/DocNo}"/>
			</FlexBox>
			<!-- WarehouseOrder: Pool Name -->
			<FlexBox class="sapUiNoMarginTop" justifyContent="SpaceBetween">
				<Text class="sapUiTinyMarginBegin sapUiNoMarginTop customTextSmall" text="{i18n>formWarehouseOrderField2}"/>
				<Text class="sapUiTinyMarginEnd sapUiNoMarginTop customTextSmall" text="{modelWarehouseOrder>/DocPoolName}"/>
			</FlexBox>
			<!-- WarehouseOrder: Task -->
			<FlexBox class="sapUiNoMarginTop" justifyContent="SpaceBetween">
				<Text class="sapUiTinyMarginBegin sapUiNoMarginTop customTextSmall" text="{i18n>formWarehouseOrderField3}"/>
				<Text class="sapUiTinyMarginEnd sapUiNoMarginTop customTextSmall" text="{modelWarehouseOrder>/WarehouseTask}"/>
			</FlexBox>
			<!-- WarehouseOrder: WPT -->
			<FlexBox class="sapUiNoMarginTop" justifyContent="SpaceBetween">
				<Text class="sapUiTinyMarginBegin sapUiNoMarginTop customTextSmall" text="{i18n>formWarehouseOrderField4}"/>
				<Text class="sapUiTinyMarginEnd sapUiNoMarginTop customTextSmall" text="{modelWarehouseOrder>/Procty}"/>
			</FlexBox>

		</VBox>
	</VBox>
</core:FragmentDefinition>
