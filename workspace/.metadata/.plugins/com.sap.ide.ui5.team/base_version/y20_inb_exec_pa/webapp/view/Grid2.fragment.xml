<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc"
	xmlns:f="sap.ui.layout.form">
	<VBox height="19rem">
			<VBox height="7.5rem">
			<FlexBox justifyContent="SpaceBetween">
				<items>
					<Text text="{i18n>Form_Dest}" class="customTextSmall sapUiTinyMarginTop"/>
					<Text text="{nextWT>/Nlpla}" class="customTextBig"/>
				</items>
			</FlexBox>
			<FlexBox justifyContent="SpaceBetween">
				<items>
					<Text text="{i18n>Form_DestSection}" class="customTextSmall sapUiTinyMarginTop"/>
					<Text text="{nextWT>/Nlber}" class="customTextLarge"/>
				</items>
			</FlexBox>
			<FlexBox justifyContent="SpaceBetween">
				<items>
					<Text text="{i18n>Form_DestType}" class="customTextSmall sapUiTinyMarginTop"/>
					<Text text="{nextWT>/Nltyp}" class="customTextLarge"/>
				</items>
			</FlexBox>
		</VBox>

	</VBox>
</core:FragmentDefinition>