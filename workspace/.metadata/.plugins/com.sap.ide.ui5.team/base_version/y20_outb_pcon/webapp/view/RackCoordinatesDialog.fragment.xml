<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <Dialog title="{i18n>dialogRackCoordinatesTitle}" id="dialogRackCoordinates" class="sapUiContentPadding">
	  <content>
        <HBox>
          <VBox width="15%">
          	<core:Icon
				src="sap-icon://message-information"
				size="3em"
				color="#0a6ed1" 
				class="">
			</core:Icon>
          </VBox>
          <VBox width="85%">
            <Title id="dialogRackCoordinatesText" text="" wrapping="true" />
            <List
            	items="{modelPopupRackCoordinates>/results}">
            	<StandardListItem title="{modelPopupRackCoordinates>StorageBin}" />
			</List>            	
          </VBox>
        </HBox> 
	  </content>
      <buttons>
        <Button class="customButton" press="onCloseRackCoordinateDialog" text="{i18n>dialogRackCoordinatesBtnClose}" width="7rem"/>
      </buttons>
    </Dialog>
</core:FragmentDefinition>