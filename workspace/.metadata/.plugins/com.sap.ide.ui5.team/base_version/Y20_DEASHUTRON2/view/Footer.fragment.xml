					<HBox width="100%" id="hbox1" justifyContent="End" alignItems="Stretch" alignContent="Stretch" class="sapUiSmallMarginEnd"/>
					<!--<MessageStrip xmlns="sap.m" showIcon="false" showCloseButton="true" id="msgStrip" text="default"/>-->
					<!--<Text id="text4911" text="innerGridBoxBody" wrapping="true"/>-->
					<Button icon="sap-icon://alert" text="{= ${message>/}.length }" visible="true" class="sapUiSmallMargin"
						press="onMessagePopoverPress"/>
				</items>
			</HBox>
		</grid:items>
	</grid:CSSGrid>
</core:FragmentDefinition>