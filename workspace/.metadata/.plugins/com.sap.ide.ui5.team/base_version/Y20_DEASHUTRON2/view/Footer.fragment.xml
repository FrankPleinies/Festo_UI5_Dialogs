			<HBox width="100%" id="hbox0" alignItems="Center" alignContent="End" class="footer_hbox">
				<items>
					<Button xmlns="sap.m" text="{i18n>BtnClose}" class="sapUiSmallMargin" id="btn_close" press="onClose"/>
