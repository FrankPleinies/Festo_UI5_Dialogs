<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout">
	<!--<grid:CSSGrid xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:grid="sap.ui.layout.cssgrid" id="innergrid1"  >-->
	<!--	<grid:items>-->
	<VBox height="45rem">
		<VBox height="50%">
			<FlexBox justifyContent="SpaceBetween">
				<items>
					<Text text="{i18n>Form_HUType}" class="customTextSmall"/>
					<Text text="{HU>/ScanHU_HUNav/HUType}" class="customTextLarge"/>
				</items>
			</FlexBox>
			<FlexBox justifyContent="SpaceBetween">
				<items>
					<Text text="{i18n>Form_AArea}" class="customTextSmall"/>
					<Text text="{HU>/ScanHU_HUNav/ActivArea}"  class="customTextLarge"/>
				</items>
			</FlexBox>
			<FlexBox justifyContent="SpaceBetween">
				<items>
					<Text text="{i18n>Form_Order}" class="customTextSmall"/>
					<Text text="{HU>/ScanHU_HUNav/WarehouseOrder}"  class="customTextLarge"/>
				</items>
			</FlexBox>
			<FlexBox justifyContent="SpaceBetween">
				<items>
					<Text text="{i18n>Form_Task}" class="customTextSmall"/>
					<Text text="{HU>/ScanHU_HUNav/WarehouseTask}"  class="customTextLarge"/>
				</items>
			</FlexBox>
			<FlexBox justifyContent="SpaceBetween">
				<items>
					<Text text="{i18n>Form_Product}" class="customTextSmall"/>
					<Text text="{HU>/ScanHU_HUNav/Product/MatNR}"  class="customTextLarge"/>
				</items>
			</FlexBox>
			<FlexBox justifyContent="SpaceBetween">
				<items>
					<Text text="{i18n>Form_Quantity}" class="customTextSmall"/>
					<Text text="{HU>/ScanHU_HUNav/Quantity/Quan} {HU>/ScanHU_HUNav/Quantity/Unit}"  class="customTextLarge"/>
				</items>
			</FlexBox>
		</VBox>
		<VBox height="35%">
			<FlexBox class="sapUiMediumMarginTop" justifyContent="SpaceBetween">
				<items>
					<Text text="{i18n>Form_ScanHU}" class="customTextSmall sapUiTinyMarginTop"/>
					<Input value="" id="scanHU" submit="onScanHU" width="100%" class="customInput"/>
				</items>
			</FlexBox>
		</VBox>
		<VBox height="15%">
			<FlexBox justifyContent="SpaceBetween" alignItems="End" class="sapUiMediumMarginTop">
				<items>
					<Button text="{i18n>BtnBack}" enabled="{ELEMENTS>/btn_back/enabled}" visible="{ELEMENTS>/btn_back/visible}" class="sapUiSmallMargin customButton"
						id="btn_back" press="onGoToAssignPressed"/>
					<Button text="{i18n>BtnRePrt}" enabled="{ELEMENTS>/btn_reprint/enabled}" visible="{ELEMENTS>/btn_reprint/visible}" class="sapUiSmallMargin customButton"
						id="btn_reprint" press="onReprint"/>
				</items>
			</FlexBox>
		</VBox>
	</VBox>
	<!--	</grid:items>-->
	<!--</grid:CSSGrid>-->
</core:FragmentDefinition>