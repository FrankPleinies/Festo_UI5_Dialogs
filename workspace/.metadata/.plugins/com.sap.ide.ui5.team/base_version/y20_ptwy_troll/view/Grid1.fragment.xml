<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc"
	xmlns:f="sap.ui.layout.form">
	<VBox height="20rem">
		<VBox height="3rem">
			<FlexBox justifyContent="SpaceBetween">
				<items>
					<Text text="{i18n>Form_Seq}" class="customTextLarge"/>
					<Text text="3" class="customTextLarge"/>
				</items>
			</FlexBox>
			<FlexBox justifyContent="SpaceBetween">
				<items>
					<Text text="{i18n>Form_Trolley}" class="customTextLarge"/>
					<Text text="TM12345" class="customTextLarge"/>
				</items>
			</FlexBox>
		</VBox>
		<VBox height="17rem">
			<FlexBox class="sapUiMediumMarginTop" justifyContent="SpaceAround" alignItems="Center" alignContent="Center">
				<items>
					<Image src="img/{ELEMENTS>/img_trolley/path}" width="410px"/>
				</items>
			</FlexBox>
		</VBox>
	</VBox>
</core:FragmentDefinition>