<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc"
	xmlns:f="sap.ui.layout.form">
	<VBox height="10rem">
		<FlexBox justifyContent="SpaceBetween">
			<Text class="customTextMedium " text="{i18n>Form_Quantity}"/>
			<StepInput class="customInput" id="stepInputQuan" change="onQuanChange" value="{= ${selectedRow>/Open}*1 }" width="11.15rem"/>
		</FlexBox>
	</VBox>
	<VBox height="5rem">
		<FlexBox justifyContent="End" alignItems="End" class="sapUiMediumMarginTop">
			<items>
				<Button text="{i18n>BtnPrint}" enabled="{ELEMENTS>/btn_print/enabled}" visible="{ELEMENTS>/btn_print/visible}" id="btn_print" press="onPrint" class="sapUiSmallMargin customButton"/>
			</items>
		</FlexBox>
	</VBox>
</core:FragmentDefinition>