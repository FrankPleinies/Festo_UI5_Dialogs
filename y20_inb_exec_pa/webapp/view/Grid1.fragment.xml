	<VBox height="19rem">
			<FlexBox justifyContent="SpaceBetween">
				<items>
					<Text text="{i18n>Form_HU}" class="customTextSmall sapUiTinyMarginTop"/>
					<Text text="{nextWT>/Vlenr}" class="customTextLarge"/>
				</items>
			</FlexBox>
			<FlexBox justifyContent="SpaceBetween">
				<items>
					<Text text="{i18n>Form_HUType}" class="customTextSmall sapUiTinyMarginTop"/>
					<Text text="{nextWT>/Hutype}" class="customTextLarge"/>
				</items>
			</FlexBox>
			<FlexBox justifyContent="SpaceBetween">
				<items>
					<Text text="{i18n>Form_Resource}" class="customTextSmall sapUiTinyMarginTop"/>
					<Text text="{nextWT>/Resource}" class="customTextLarge"/>
				</items>
			</FlexBox>
	</VBox>
</core:FragmentDefinition>