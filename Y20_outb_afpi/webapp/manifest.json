{
	"_version": "1.1.0",
	"sap.app": {
		"_version": "1.1.0",
		"id": "y20_outb_afpi",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"resources": "resources.json",
		"ach": "ach",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		},
		"dataSources": {
			"mainService": {
				"type": "OData",
				"settings": {
					"odataVersion": "2.0"
				},
				"uri": "/sap/opu/odata/sap/Y20_EWM_AFTER_PICK_DROPOFF_SRV"
			}
		}		
	},
	"sap.ui": {
		"_version": "1.1.0",
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"_version": "1.1.0",
		"flexEnabled": false,
		"rootView": {
			"viewName": "y20_outb_afpi.view.MainScreen",
			"type": "XML",
			"async": true,
			"id": "MainScreen"
		},
		"dependencies": {
			"minUI5Version": "1.65.6",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "y20_outb_afpi.i18n.i18n"
				}
			},
			"": {
				"dataSource": "mainService"
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/newStyle.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "y20_outb_afpi.view",
				"controlAggregation": "pages",
				"controlId": "idAppControl",
				"clearControlAggregation": false
			},
			"routes": [{
				"name": "RouteMainScreen",
				"pattern": "RouteMainScreen",
				"target": ["TargetMainScreen"]
			}],			
			"targets": {
				"TargetMainScreen": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewId": "MainScreen",
					"viewName": "MainScreen"
				}
			}
		}
	}
}