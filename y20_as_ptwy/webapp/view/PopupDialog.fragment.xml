<core:FragmentDefinition xmlns="sap.m" 
    xmlns:core="sap.ui.core" 
    xmlns:table="sap.ui.table">
    <Dialog title="Information" id="popupDialog" class="sapUiSizeCozy sapUiContentPadding">

        <table:Table id="uiTablePopup" rows="{modelPopDocHeadNav>/results}" selectionBehavior="RowOnly" selectionMode="Single" visibleRowCount="6" itemsenableColumnReordering="false" width="auto" class="sapUiSmallMarginTop">
            <table:columns>
                <table:Column>
                    <Label text="Inb. Deliv."/>
                    <table:template>
                        <Text text="{= ${modelPopDocHeadNav>DocNo}*1 }"/>
                    </table:template>
                </table:Column>
                <table:Column>
                    <Label text="Mat. No."/>
                    <table:template>
                        <Text text="{modelPopDocHeadNav>MatNr}"/>
                    </table:template>
                </table:Column>
                <table:Column>
                    <Label text="Mat. Descr."/>
                    <table:template>
                        <Text text="{modelPopDocHeadNav>MatTXT}"/>
                    </table:template>
                </table:Column>
                <table:Column>
                    <Label text="Quantity"/>
                    <table:template>
                        <Text text="{modelPopDocHeadNav>Quan/Quan} {modelPopDocHeadNav>Quan/Unit}"/>
                    </table:template>
                </table:Column>
            </table:columns>
        </table:Table>
        <buttons>
            <Button text="Confirm" press="onConfirmDialog" />
        </buttons>
    </Dialog>
</core:FragmentDefinition>