<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout" xmlns:table="sap.ui.table">
	<VBox height="17rem">
			<table:Table cellClick="onCellClick" id="tblDeliveryCountedContent" selectionBehavior="RowOnly" noData="keine Daten" rowActionCount="1" rowSelectionChange="onSelect" visibleRowCount="5"
				selectionMode="Single" rows="{BinContent>/results}"  enableColumnReordering="false" width="auto" enableCellFilter="true" sort="sortTable">
					<table:rowSettingsTemplate>
						<table:RowSettings highlight="{  
									path: 'uiTableModelCounted>Result',
									formatter: '.formatter.setHighlight'}" />
					</table:rowSettingsTemplate>
				<table:columns >
					<table:Column >
						<Label text="{i18n>Table2_Col1}" class="customTableHeaderSmall"/>
						<table:template class="customTableRowBackgroundOK">
							<Text text="{BinContent>Compartment}" wrapping="false" class="customTableRowSmall"/>
						</table:template>
					</table:Column>
					<table:Column sortProperty="Product/MATNR">
						<Label text="{i18n>Table2_Col2}" class="customTableHeaderSmall"/>
						<table:template>
							<Text text="{BinContent>Product/MATNR}" wrapping="false" class="customTableRowSmall"/>
						</table:template>
					</table:Column>
					<table:Column sortProperty="TaskCount">
						<Label text="{i18n>Table2_Col3}" class="customTableHeaderSmall"/>
						<table:template>
							<Text text="{BinContent>Product/MAKTX}" wrapping="false" class="customTableRowSmall"/>
						</table:template>
					</table:Column>
					<table:Column>
						<Label text="{i18n>Table2_Col4}" class="customTableHeaderSmall"/>
						<table:template>
							<Text text="{BinContent>Quant/Quan}" wrapping="false" class="customTableRowSmall"/>
						</table:template>
					</table:Column>
					<table:Column>
						<Label text="{i18n>Table2_Col5}" class="customTableHeaderSmall"/>
						<table:template>
							<Text text="{BinContent>Quant/Unit}" wrapping="false" class="customTableRowSmall"/>
						</table:template>
					</table:Column>
					<table:Column>
						<Label text="" class="customTableHeaderSmall"/>
						<table:template>
							<Button icon="sap-icon://print" enabled="{ELEMENTS>/btn_reprint/enabled}" visible="{ELEMENTS>/btn_reprint/visible}" id="btn_reprint" press="onPressReprint" text="" wrapping="false"/>
						</table:template>
					</table:Column>
				</table:columns>
			</table:Table>
	</VBox>

</core:FragmentDefinition>