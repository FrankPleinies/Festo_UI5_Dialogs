<core:FragmentDefinition xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:l="sap.ui.layout"
	xmlns:table="sap.ui.table"
	xmlns:form="sap.ui.layout.form"
	xmlns="sap.m">
	<VBox justifyContent="Start">
		<FlexBox justifyContent="SpaceBetween" width="auto">
			<VBox width="100%">
				<table:Table id="uiTableThird" rows="{modelTrolleyNav>/results}" selectionMode="None" visibleRowCount="7" enableColumnReordering="false" width="100%">
					<table:columns>
						<table:Column width="3.5rem">
							<Label class="customTableHeaderSmall" text="{i18n>posistionShort}"/>
							<table:template>
								<Text class="customTableRowSmall" text="{modelTrolleyNav>LOGPOS}"/>
							</table:template>
						</table:Column>
						<table:Column width="7rem">
							<Label class="customTableHeaderSmall" tooltip="{i18n>pickHU}" text="{i18n>hu}"/>
							<table:template>
								<VBox height="20px">
									<Text class="customTableRowSmall" text="{modelTrolleyNav>HU}"/>
								</VBox>
							</table:template>
						</table:Column>
						<table:Column>
							<Label class="customTableHeaderSmall" tooltip="{i18n>poolName}" text="{i18n>pool}"/>
							<table:template>
								<VBox height="20px">
									<Text class="customTableRowSmall" text="{modelTrolleyNav>POOLNAME}"/>
								</VBox>
							</table:template>
						</table:Column>
						<table:Column>
							<Label class="customTableHeaderSmall" tooltip="{i18n>product}" text="{i18n>product}"/>
							<table:template>
								<VBox height="20px">
									<Text class="customTableRowSmall" text="{modelTrolleyNav>MATNR}"/>
								</VBox>
							</table:template>
						</table:Column>
						<table:Column>
							<Label class="customTableHeaderSmall" text="{i18n>quanShort}"/>
							<table:template>
								<VBox height="20px">
									<Text class="customTableRowSmall" text="{modelTrolleyNav>QUAN/QUAN} {modelcreatedHUNav>QUAN/UNIT}"/>
								</VBox>
							</table:template>
						</table:Column>

					</table:columns>
				</table:Table>
			</VBox>
		</FlexBox>

	</VBox>
</core:FragmentDefinition>