<core:FragmentDefinition xmlns="sap.m"
    xmlns:l="sap.ui.layout"
    xmlns:f="sap.ui.layout.form"
    xmlns:core="sap.ui.core">
    <Dialog title="{i18n>changeQueue}" id="popupDialogChangeQueue" class="sapUiSizeCozy sapUiContentPadding" afterClose="afterCloseChangeQueue" afterOpen="afterOpenChangeQueue">
        <f:Form editable="true">
            <f:layout>
                <f:ResponsiveGridLayout labelSpanXL="3" labelSpanL="3" labelSpanM="3" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="4" emptySpanL="4" emptySpanM="4" emptySpanS="0" columnsXL="1" columnsL="1" columnsM="1" singleContainerFullSize="false" />
            </f:layout>
            <f:formContainers>
                <f:FormContainer>
                    <f:formElements>
                        <f:FormElement>
                            <List class="sapUiMediumMarginBottom customListBorder" showSeparators="None">
                                <items>
                                    <StandardListItem title="{i18n>warehouseOrder}" info="{modelToNav>/TANUM}" />
                                    <StandardListItem title="{i18n>queue}" info="{modelToNav>/QUEUE}"/>
                                </items>
                            </List>
                        </f:FormElement>
                        <f:FormElement>
                            <VBox>
                                <Label text="{i18n>changeQueue}:" design=""/>
                                <Input submit="onChangeQueueSubmit" id="changeQueueInput"/>
                            </VBox>
                        </f:FormElement>
                    </f:formElements>
                </f:FormContainer>
            </f:formContainers>
        </f:Form>
        <buttons>
            <Button text="{i18n>close}" press="onChangeQueueClose"/>
        </buttons>
    </Dialog>
</core:FragmentDefinition>