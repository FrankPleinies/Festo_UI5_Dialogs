<core:FragmentDefinition xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:l="sap.ui.layout"
	xmlns:table="sap.ui.table"
	xmlns:form="sap.ui.layout.form"
	xmlns="sap.m">
	<VBox justifyContent="Start">
		<FlexBox justifyContent="SpaceBetween">
			<VBox width="100%">
				<table:Table id="uiTableSixth" rows="{modelTrolleyNav>/results}" selectionMode="None" visibleRowCount="5" enableColumnReordering="false" width="auto">
					<table:columns>
						<table:Column>
							<Label class="customTableHeaderSmall" text="{i18n>no}"/>
							<table:template>
								<Text class="customTableRowSmall" text="{modelTrolleyNav>SeqNo}"/>
							</table:template>
						</table:Column>
						<table:Column>
							<Label class="customTableHeaderSmall" text="{i18n>trolley}"/>
							<table:template>
								<Text class="customTableRowSmall" text="{modelTrolleyNav>Trolley}"/>
							</table:template>
						</table:Column>
						<table:Column>
							<Label class="customTableHeaderSmall" text="{i18n>actArea}"/>
							<table:template>
								<Text class="customTableRowSmall" text="{modelTrolleyNav>ActivArea}"/>
							</table:template>
						</table:Column>
						<table:Column>
							<Label class="customTableHeaderSmall" text="{i18n>ass}"/>
							<table:template>
								<Text class="customTableRowSmall" text="{modelTrolleyNav>Ass}"/>
							</table:template>
						</table:Column>
						<table:Column>
							<Label class="customTableHeaderSmall" text="{i18n>nas}"/>
							<table:template>
								<Text text="{modelTrolleyNav>NAs}"/>
							</table:template>
						</table:Column>
					</table:columns>
				</table:Table>
			</VBox>
		</FlexBox>
		<FlexBox class="sapUiSmallMarginTop" justifyContent="End">
			<Button class="customButton" press="onGoToAssignPressed" text="{i18n>goToAssign}" width="10rem"/>
		</FlexBox>
	</VBox>
</core:FragmentDefinition>