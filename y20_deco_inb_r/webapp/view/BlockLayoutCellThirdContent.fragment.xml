<core:FragmentDefinition xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:l="sap.ui.layout"
	xmlns:table="sap.ui.table"
	xmlns:form="sap.ui.layout.form"
	xmlns="sap.m">
	<VBox justifyContent="Start">
		<FlexBox justifyContent="SpaceBetween" width="auto">
			<VBox width="100%">
				<table:Table id="uiTableThird" rows="{modelcreatedHUNav>/results}" selectionMode="MultiToggle" visibleRowCount="7" enableColumnReordering="false" width="100%">
					<table:columns>
						<table:Column width="3rem">
							<Label class="customTableHeaderSmall" text="{i18n>itemShort}"/>
							<table:template>
								<Text class="customTableRowSmall" text="{modelcreatedHUNav>ItemNo}"/>
							</table:template>
						</table:Column>
						<table:Column width="3rem">
							<Label class="customTableHeaderSmall" tooltip="{i18n>sequence}" text="{i18n>seqShort}"/>
							<table:template>
								<Text class="customTableRowSmall" text="{modelcreatedHUNav>SeqNo}"/>
							</table:template>
						</table:Column>
						<table:Column width="4rem">
							<Label class="customTableHeaderSmall" tooltip="{i18n>product}" text="{i18n>productShort}"/>
							<table:template>
								<VBox height="20px">
									<Text class="customTableRowSmall" text="{modelcreatedHUNav>Product/MATNR}"/>
								</VBox>
							</table:template>
						</table:Column>
						<table:Column width="5rem">
							<Label class="customTableHeaderSmall" text="{i18n>quantity}"/>
							<table:template>
								<VBox height="20px">
									<Text class="customTableRowSmall" text="{modelcreatedHUNav>Quantity/Quan} {modelcreatedHUNav>Quantity/Unit}"/>
								</VBox>
							</table:template>
						</table:Column>
						<table:Column>
							<Label class="customTableHeaderSmall" text="{i18n>hu}"/>
							<table:template>
								<VBox height="20px">
									<Text class="customTableRowSmall" text="{modelcreatedHUNav>HU/HUIdent}"/>
								</VBox>
							</table:template>
						</table:Column>
						<table:Column>
							<Label class="customTableHeaderSmall" text="{i18n>huType}"/>
							<table:template>
								<VBox height="20px">
									<Text class="customTableRowSmall" text="{modelcreatedHUNav>HU/HUType}"/>
								</VBox>
							</table:template>
						</table:Column>
						<table:Column>
							<Label class="customTableHeaderSmall" text="{i18n>aa}"/>
							<table:template>
								<VBox height="20px">
									<Text class="customTableRowSmall" text="{modelcreatedHUNav>ActivityArea}"/>
								</VBox>
							</table:template>
						</table:Column>
					</table:columns>
				</table:Table>
			</VBox>
		</FlexBox>
		<FlexBox>
			<Button class="customButton sapUiSmallMarginTop" press="onUndoPressed" text="{i18n>undo}" width="7rem"/>
		</FlexBox>
	</VBox>
</core:FragmentDefinition>