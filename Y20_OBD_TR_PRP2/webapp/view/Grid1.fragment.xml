<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout">
	<!--<grid:CSSGrid xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:grid="sap.ui.layout.cssgrid" id="innergrid1"  >-->
	<!--	<grid:items>-->
	<VBox height="45rem">
		<VBox height="60%">
			<FlexBox justifyContent="SpaceBetween">
				<items>
					<Text text="{i18n>Form_HUType}" class="customTextSmall"/>
					<Text text="{HU>/ScanHU_HUNav/HUType}" class="customTextLarge"/>
				</items>
			</FlexBox>
			<FlexBox justifyContent="SpaceBetween">
				<items>
					<Text text="{i18n>Form_AArea}" class="customTextMedium"/>
					<Text text="{HU>/ScanHU_HUNav/ActivArea}"  class="customTextSmall"/>
				</items>
			</FlexBox>
			<FlexBox justifyContent="SpaceBetween">
				<items>
					<Text text="{i18n>Form_Cons}" class="customTextMedium"/>
					<Text text="{HU>/ScanHU_HUNav/PickCons}"  class="customTextSmall"/>
				</items>
			</FlexBox>
			<FlexBox justifyContent="SpaceBetween">
				<items>
					<Text text="{i18n>Form_Pool}" class="customTextMedium"/>
					<Text text="{HU>/ScanHU_HUNav/PoolMix}"  class="customTextSmall"/>
				</items>
			</FlexBox>
			<FlexBox justifyContent="SpaceBetween">
				<items>
					<Text text="{i18n>Form_Who}" class="customTextMedium"/>
					<Text text="{HU>/ScanHU_HUNav/WarehouseOrder}"  class="customTextSmall"/>
				</items>
			</FlexBox>
			<!-- <FlexBox justifyContent="SpaceBetween">
				<items>
					<Text text="{i18n>Form_Task}" class="customTextMedium"/>
					<Text text="{HU>/ScanHU_HUNav/Quantity/Quan} {HU>/ScanHU_HUNav/Quantity/Unit}"  class="customTextSmall"/>
				</items>
			</FlexBox> -->
		</VBox>
		<VBox height="25%">
			<FlexBox class="sapUiMediumMarginTop" justifyContent="SpaceBetween">
				<items>
					<Text text="{i18n>Form_ScanHU}" class="sapUiSmallMarginTop customTextMedium"/>
					<Input value="" id="scanHU" submit="onScanHU" width="100%" class="customInput"/>
				</items>
			</FlexBox>
		</VBox>
		<VBox height="15%">
			<FlexBox justifyContent="SpaceBetween" alignItems="End" class="sapUiMediumMarginTop">
				<items>
					<Button text="{i18n>BtnBack}" enabled="{ELEMENTS>/btn_back/enabled}" visible="{ELEMENTS>/btn_back/visible}" class="sapUiSmallMargin customButton"
						id="btn_back" press="onGoToAssignPressed"/>
					<Button text="{i18n>BtnInfo}" enabled="{ELEMENTS>/btn_info/enabled}" visible="{ELEMENTS>/btn_info/visible}" class="sapUiSmallMargin customButton" id="btn_info" press="onInfo"/>
				</items>
			</FlexBox>
		</VBox>
	</VBox>
	<!--	</grid:items>-->
	<!--</grid:CSSGrid>-->
</core:FragmentDefinition>